@model IEnumerable<DeanERP.Models.OrderFood.OrderStoreModel>

@{
    ViewBag.Title = "ReadOrderStore";
}

<h2>ReadOrderStore</h2>
<p>
    @Html.ActionLink("建立新店家", "CreateOrderStore")
</p>

@using (Html.BeginForm())
{
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.STORE_NAME)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.STORE_TYPE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.STORE_DELIVERY)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.STORE_BLICKLIST)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.STORE_FREQ)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.STORE_FINALDATE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.STORE_TEL1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.STORE_SUB)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.STORE_MENUURL)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink(@item.STORE_NAME, "/ReadDetailOrderStore", new { storeId = item.STORE_ID }) 
            <!--<input type="submit" value="item.STORE_NAME" name="storeName" formaction="/OrderStore/ReadDetailOrderStore" formmethod="post" class="text_button">-->
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.STORE_TYPE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.STORE_DELIVERY)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.STORE_BLICKLIST)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.STORE_FREQ)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.STORE_FINALDATE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.STORE_TEL1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.STORE_SUB)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.STORE_MENUURL)
        </td>
        <td>
            @Html.ActionLink("Edit", "/EditOrderStore", new { storeId = item.STORE_ID }) |
            @Html.ActionLink("Delete", "Delete", new { storeId = item.STORE_ID })
        </td>
    </tr>
    }
</table>
}
